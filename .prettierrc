{
  "printWidth": 120,
  "singleQuote": true,
  "useTabs": false,
  "tabWidth": 2,
  "semi": true,
  "bracketSpacing": true,
  "endOfLine": "auto",
  "trailingComma": "es5",
  "arrowParens": "always",
  "proseWrap": "preserve",
  "quoteProps": "preserve",
  "jsxSingleQuote": false,
  "bracketSameLine": false,
  "htmlWhitespaceSensitivity": "css",
  "embeddedLanguageFormatting": "auto",
  "overrides": [
    {
      "files": ["*.html", ".scss"],
      "options": {
        "printWidth": 80
      }
    }
  ]
}
